    "angular-animate": "^1.5.7",
    "angular-resource": "^1.5.3",
    "angular-route": "^1.5.6",
    "angular-stripe": "^4.2.12",
    "angular-ui-router": "^1.0.0-alpha.4",
    "bootstrap": "^3.3.6",
    "babel-cli": "^6.0.0",
    "babel-core": "6.1.18",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.0",
    "babel-require": "^1.0.1",
    "bluebird": "^3.4.1",
    "body-parser": "1.12.4",
    "dotenv": "^2.0.0",
    "express": "4.12.3",
    "express-session": "1.11.2",
    "mongoose": "4.0.3",
    "passport": "0.2.2",
