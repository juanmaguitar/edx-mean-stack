{
  "name": "edx-mean-stack-angular",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "server:start": "babel-node --presets es2015 server/app",
    "server:nodemon:start": "nodemon --exec npm run server:start",
    "server:nodemon:test": "nodemon --exec npm server/test",
    "server:test": "mocha --compilers js:babel-register server/test",
    "server:check-credentials": "mocha server/test/part1.js",
    "server:watch": "gulp watch",
    "client:dist:fonts": "mv dist/glyphicons* dist/css",
    "client:dist": "git add dist && git commit -am \"dist $(date +'%h-%m %s')\" &&  git push",
    "client:gh-pages": "git subtree push --prefix dist origin gh-pages",
    "client:deploy": "npm run client:build && npm run client:dist && npm run client:gh-pages",
    "client:build": "rimraf dist && webpack --config ./webpack.production.config.js --bail --progress --profile && npm run client:dist:fonts",
    "client:dev-server": "webpack-dev-server --history-api-fallback --inline --progress",
    "client:start": "npm run client:dev-server",
    "client:watch": "npm run client:watch:dev",
    "client:watch:dev": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "client:watch:dev:hmr": "npm run client:watch:dev -- --hot",
    "client:watch:prod": "webpack --watch --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",

    "start": "NODE_ENV=production PORT=3000 node server/app",
    "server": "node server/app",
    "build": "rimraf public/* && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "test": "karma start"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.9.0",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^1.1.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "3.8.11",
    "gulp-mocha": "2.0.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "2.2.4",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "protractor": "^3.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "stats-webpack-plugin": "^0.2.2",
    "style-loader": "^0.13.1",
    "superagent": "1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^1.2.0"
  },
  "dependencies": {
    "http-status": "0.1.8",
    "underscore": "1.5.2",
